#include <stdio.h>

int main() {
    int n, i, blocks[10];

    printf("Enter number of blocks: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("Enter block number: ");
        scanf("%d", &blocks[i]);
    }

    printf("Linked Allocation:\n");
    for(i = 0; i < n-1; i++)
        printf("%d -> ", blocks[i]);
    printf("%d\n", blocks[n-1]);

    return 0;
}
