#include <stdio.h>

int main() {
    int n, i, index[10];

    printf("Enter number of blocks: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("Enter block number %d: ", i+1);
        scanf("%d", &index[i]);
    }

    printf("Index Block contains:\n");
    for(i = 0; i < n; i++)
        printf("%d ", index[i]);

    return 0;
}
